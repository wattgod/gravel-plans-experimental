    <SteadyState Duration="60" Power="1.15" Cadence="100"/>
    <SteadyState Duration="840" Power="0.82" Cadence="90"/>
    <SteadyState Duration="480" Power="0.55"/>
    <SteadyState Duration="60" Power="1.15" Cadence="100"/>
    <SteadyState Duration="840" Power="0.82" Cadence="90"/>
    <SteadyState Duration="3600" Power="0.70" Cadence="85"/>
